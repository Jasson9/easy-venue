{
  "private": true,
  "description": "An example project for Auth.js with SvelteKit",
  "repository": "https://github.com/nextauthjs/sveltekit-auth-example",
  "bugs": {
    "url": "https://github.com/nextauthjs/next-auth/issues"
  },
  "homepage": "https://sveltekit-auth-example.vercel.app",
  "scripts": {
    "dev": "vite dev",
    "build": "prisma generate && vite build",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "postinstall": "prisma generate"
  },
  "devDependencies": {
    "@iconify/svelte": "^4.0.2",
    "@sveltejs/adapter-auto": "3.2.0",
    "@sveltejs/kit": "2.5.7",
    "@sveltejs/vite-plugin-svelte": "^3.1.0",
    "@types/node": "^20.14.2",
    "autoprefixer": "^10.4.19",
    "postcss": "^8.4.38",
    "prisma": "^5.15.0",
    "svelte": "4.2.15",
    "svelte-check": "3.6.9",
    "tailwindcss": "^3.4.3",
    "typescript": "5.4.5",
    "vite": "5.2.10"
  },
  "dependencies": {
    "@auth/prisma-adapter": "^2.2.0",
    "@auth/sveltekit": "latest",
    "@popperjs/core": "^2.11.8",
    "@prisma/client": "^5.15.0",
    "boxicons": "^2.1.4",
    "cuid": "^3.0.0",
    "dotenv": "^16.4.5",
    "flowbite": "^2.3.0",
    "flowbite-svelte": "^0.46.1",
    "tailwind-merge": "^2.3.0"
  },
  "type": "module"
}
